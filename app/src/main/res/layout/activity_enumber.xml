<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/enumber"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ENumber">

    <EditText
        android:id="@+id/editText2"
        android:layout_width="299dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="146dp"
        android:ems="10"
        android:fontFamily="@font/roboto_thin"
        android:gravity="center_horizontal"
        android:inputType="phone"
        android:textColor="#CBE9D7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="532dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="189dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="29dp"
        android:background="@android:color/transparent"
        android:textAllCaps="false"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="629dp" />

    <TextView
        android:id="@+id/num0"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/editText2"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="170dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="165dp" />

    <TextView
        android:id="@+id/num1"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/editText2"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="213dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/num2"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/editText2"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="262dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="259dp" />

    <TextView
        android:id="@+id/num3"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/num4"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="306dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="310dp" />

    <TextView
        android:id="@+id/num4"
        android:layout_width="260dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/editText2"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="350dp"
        android:fontFamily="@font/roboto_thin"
        android:text="0812345678"
        android:textColor="#CBE9D7"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="359dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignEnd="@+id/editText2"
        android:layout_alignBottom="@+id/num0"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="0dp"
        android:background="@android:color/transparent"
        android:onClick="del1"
        tools:layout_editor_absoluteX="333dp"
        tools:layout_editor_absoluteY="161dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBottom="@+id/num1"
        android:layout_marginStart="-15dp"
        android:layout_marginBottom="-1dp"
        android:layout_toEndOf="@+id/num1"
        android:background="@android:color/transparent"
        android:onClick="del2"
        tools:layout_editor_absoluteX="333dp"
        tools:layout_editor_absoluteY="211dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_below="@+id/num1"
        android:layout_alignEnd="@+id/editText2"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:background="@android:color/transparent"
        android:onClick="del3"
        tools:layout_editor_absoluteX="333dp"
        tools:layout_editor_absoluteY="259dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignBaseline="@+id/num3"
        android:layout_marginStart="-10dp"
        android:layout_toEndOf="@+id/num3"
        android:background="@android:color/transparent"
        android:onClick="del4"
        tools:layout_editor_absoluteX="333dp"
        tools:layout_editor_absoluteY="306dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:layout_alignTop="@+id/num4"
        android:layout_marginStart="-12dp"
        android:layout_marginTop="-1dp"
        android:layout_toEndOf="@+id/num0"
        android:background="@android:color/transparent"
        android:onClick="del5"
        tools:layout_editor_absoluteX="333dp"
        tools:layout_editor_absoluteY="359dp" />
</RelativeLayout>